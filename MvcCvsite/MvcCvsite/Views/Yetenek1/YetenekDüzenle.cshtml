@model MvcCvsite.Models.Entity.TblYeteneklerim
@{
    ViewBag.Title = "YetenekDüzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yetenek Düzenleme İşlemleri</h4>
<br />

<form method="post">

    @Html.Label("Yetenek Açıklması")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })

    <br />
    <button class="btn btn-info">Güncelle</button>
</form>

