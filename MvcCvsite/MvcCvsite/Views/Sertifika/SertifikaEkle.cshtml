@model MvcCvsite.Models.Entity.TblSertifikalar
@{
    ViewBag.Title = "SertifikaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Sertifika Ekleme İşlemi</h4>
<br />

<form method="post">

    @Html.Label("Sertifika Adı")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
