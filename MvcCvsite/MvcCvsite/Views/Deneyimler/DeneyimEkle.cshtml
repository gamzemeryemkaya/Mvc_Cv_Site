
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br/>
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br/>

<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlık yazınız" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi" />
    <br />
    <textarea type="text" name="Aciklama" class="form-control" placeholder="Açıklama giriniz" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>